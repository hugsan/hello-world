import java.util.Scanner;
public class Week2_Problem1 {
    public static void main(String[] args){
        Scanner input = new Scanner (System.in);
        int num = input.nextInt();
        int even = num%2;
        if(even==0){
            System.out.println("Even");
        }
        else{
            System.out.println("Odd");
        }
    }

}
